<%= form_for(@sideproject) do |f| %>
  <h5>Project Title</h5>
  <%= f.text_field :title, autofocus: true, class:"form-control"%>

  <h5>Project date</h5>
  <%= f.text_field :projectdate, autofocus: true, class:"form-control"%>

  <h5>Cover image</h5>
  <%= f.text_field :coverimage, autofocus: true, class:"form-control"%>

  <h5>Brief Intro</h5>
  <%= f.text_field :briefintro, autofocus: true, class:"form-control"%>

  <h5>Overview</h5>
  <%= f.text_area :overview, autofocus: true, class:"form-control"%>

  <h5>Decription</h5>
  <%= f.text_area :description, autofocus: true, class:"form-control"%>
  <br/>
  <%= f.submit "Save", class:"btn btn-success" %>
  <%= link_to "Unsave", sideprojects_path, class:"btn btn-danger" %>
<% end %>
